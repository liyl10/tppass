<struts>
	<package name="role" extends="admin" namespace="/api/role">
    	<action name="role_*" class="com.hopsun.scenter.api.role.action.RoleAction"  method="{1}">
       	 	<result name="{1}">/api_scenter/role/role_{1}.jsp</result>
